{
  "metadata": [
    {
      "src": [
        {
          "files": [ "CopperSharp/bin/Release/net6.0/CopperSharp.dll" ]
        }
      ],
      "dest": "obj/docs"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/docs",
        "dest": "api"
      },
      {
        "files": ["guides/**/*.md", "*.md", "toc.yml"]
      }
    ],
    "resource": [
      {
        "files": [ "articles/images/**"]
      }
    ],
    "globalMetadata": {
      "_appTitle": "CopperSharp",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "docs",
    "template": [
      "default",
      "templates/darkfx"
    ]
  }
}